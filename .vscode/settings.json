        "**/.svn": true,
        "**/.venv/": true,
        "**/CVS": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/custom_components/": true,
        "**/deps": true,
        "**/home-assistant.log*": true,
        "**/home-assistant_v2.db*": true,
        "**/node_modules/": true,
        "**/package-lock.json": true,
        "**/poetry.lock": true,
        "**/themes/": true,
        "**/ui_lovelace_minimalist/dashboard/adaptive-dash/": false,
