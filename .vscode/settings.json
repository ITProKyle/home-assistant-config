        "**/binary_sensors/**/*.y*ml": "home-assistant",
        "**/blueprints/**/*.y*ml": "home-assistant",
        "**/configuration.y*ml": "home-assistant",
        "**/groups.y*ml": "home-assistant",
        "**/input_boolean.y*ml": "home-assistant",
